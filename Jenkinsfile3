pipeline{
           agent {docker { image 'maven:3-amazoncorretto-17-debian' }}        
    }
    stages{
        stage('check current directory'){
            steps{
                sh 'pwd'
                sh 'whoami'
                sh "mkdir \$(date '+%Y%m%d_%H%M%S')"
                sh 'python3 --version'
                sh 'npm --version'
            }
        }
    }
}